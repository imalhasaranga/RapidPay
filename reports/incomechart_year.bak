	<queryString><![CDATA[select sum(Income) as Income,Month  from(

      SELECT sum(class_fees) as Income, SUBSTRING(Date_time,6,2) as Month 
      FROM `payment_invoice_student`  where Date_time like concat($P{year},'%') && payment_id in (SELECT payment_id FROM payment_invoice_student where  pay_type = 'monthlypayment' )    group by  SUBSTRING(Date_time,6,2) 

      union

     SELECT sum(class_fees), SUBSTRING(Date_time,6,2) as Month 
    FROM `payment_invoice_student`  where Date_time like concat($P{year},'%')  && payment_id in (SELECT payment_id FROM payment_invoice_student where pay_type = 'Admission' group by user_student_id)   group by  SUBSTRING(Date_time,6,2)


) a group by Month order by Month asc]]></queryString>

